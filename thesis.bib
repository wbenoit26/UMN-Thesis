@article{GoVe2017,
	doi = {10.3847/2041-8213/aa8f41},
	year = 2017,
	month = {oct},
	publisher = {American Astronomical Society},
	volume = {848},
	number = {2},
	pages = {L14},
	author = {A. Goldstein and P. Veres and E. Burns and M. S. Briggs and R. Hamburg and D. Kocevski and C. A. Wilson-Hodge and R. D. Preece and S. Poolakkil and O. J. Roberts and C. M. Hui and V. Connaughton and J. Racusin and A. von Kienlin and T. Dal Canton and N. Christensen and T. Littenberg and K. Siellez and L. Blackburn and J. Broida and E. Bissaldi and W. H. Cleveland and M. H. Gibby and M. M. Giles and R. M. Kippen and S. McBreen and J. McEnery and C. A. Meegan and W. S. Paciesas and M. Stanbro},
	title = {An Ordinary Short Gamma-Ray Burst with Extraordinary Implications: Fermi-{GBM} Detection of {GRB} 170817A},
	journal = {The Astrophysical Journal},
	abstract = {On 2017 August 17 at 12:41:06 UTC the Fermi  Gamma-ray Burst Monitor (GBM) detected and triggered on the short gamma-ray burst (GRB) 170817A. Approximately 1.7Â s prior to this GRB, the Laser Interferometer Gravitational-wave Observatory triggered on a binary compact merger candidate associated with the GRB. This is the first unambiguous coincident observation of gravitational waves and electromagnetic radiation from a single astrophysical source and marks the start of gravitational-wave multi-messenger astronomy. We report the GBM observations and analysis of this ordinary short GRB, which extraordinarily confirms that at least some short GRBs are produced by binary compact mergers.}
}
@article{SaFe2017,
	doi = {10.3847/2041-8213/aa8f94},
	year = 2017,
	month = {oct},
	publisher = {American Astronomical Society},
	volume = {848},
	number = {2},
	pages = {L15},
	author = {V. Savchenko and C. Ferrigno and E. Kuulkers and A. Bazzano and E. Bozzo and S. Brandt and J. Chenevez and T. J.-L. Courvoisier and R. Diehl and A. Domingo and L. Hanlon and E. Jourdain and A. von Kienlin and P. Laurent and F. Lebrun and A. Lutovinov and A. Martin-Carrillo and S. Mereghetti and L. Natalucci and J. Rodi and J.-P. Roques and R. Sunyaev and P. Ubertini},
	title = {{INTEGRAL} Detection of the First Prompt Gamma-Ray Signal Coincident with the Gravitational-wave Event {GW}170817},
	journal = {The Astrophysical Journal},
	abstract = {We report the INTernational Gamma-ray Astrophysics Laboratory (INTEGRAL) detection of the short gamma-ray burst GRB 170817A (discovered by Fermi-GBM) with a signal-to-noise ratio of 4.6, and, for the first time, its association with the gravitational waves (GWs) from binary neutron star (BNS) merging event GW170817 detected by the LIGO and Virgo observatories. The significance of association between the gamma-ray burst observed by INTEGRAL and GW170817 is 3.2Ï, while the association between the Fermi-GBM and INTEGRAL detections is 4.2Ï. GRB 170817A was detected by the SPI-ACS instrument about 2 s after the end of the GW event. We measure a fluence of (1.4Â Â±Â 0.4Â Â±Â 0.6)Â ÃÂ 10â7 erg cmâ2 (75â2000 keV), where, respectively, the statistical error is given at the 1Ï confidence level, and the systematic error corresponds to the uncertainty in the spectral model and instrument response. We also report on the pointed follow-up observations carried out by INTEGRAL, starting 19.5 hr after the event, and lasting for 5.4 days. We provide a stringent upper limit on any electromagnetic signal in a very broad energy range, from 3 keV to 8 MeV, constraining the soft gamma-ray afterglow flux to <7.1Â ÃÂ 10â11 erg cmâ2Â sâ1 (80â300 keV). Exploiting the unique capabilities of INTEGRAL, we constrained the gamma-ray line emission from radioactive decays that are expected to be the principal source of the energy behind a kilonova event following a BNS coalescence. Finally, we put a stringent upper limit on any delayed bursting activity, for example, from a newly formed magnetar.}
}
